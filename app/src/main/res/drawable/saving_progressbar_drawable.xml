<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:endColor="@color/photomovie_progress_back"
                android:startColor="@color/photomovie_progress_back"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/photomovie_progress_back"
                    android:startColor="@color/photomovie_progress_back"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/photomovie_progress_down"
                    android:startColor="@color/photomovie_progress_down"/>
            </shape>
        </clip>
    </item>
</layer-list>